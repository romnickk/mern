

{
    "version": 2,
    "builds": [
        {
            "src":"*.js",
            "use":"@vercel/node"
        }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      },
      {
        "src": "/api/.*",
        "dest": "/api/productRoutes.js"
      },
      {
        "src":"/[^.]",
        "dest": "/",
        "status":200
      }
    ]
  }